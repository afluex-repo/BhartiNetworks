@model BhartiNetwork.Models.Admin

@{
    ViewBag.Title = "InternShipApplication";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<style>
    .form-group label {
        vertical-align: sub !important;
    }
</style>
@using (Html.BeginForm("InternShipApplication", "Admin", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-4">
                            <img src="~/assets/img/logo.png" class="logo img-fluid" style="width:200px;" alt="">
                        </div>
                        <div class="col-md-4">
                            <h4>Internship Application Form</h4>
                            <p>(Fill all fields in Block letters)</p>
                        </div>
                        <div class="col-md-4 text-right">
                            @*<img src="~/AdminCss/assets/images/user.png" class="img-fluid" style="height:100px;" />*@
                            <span style="color: red">*</span>  <input type="file" name="PostedFile" id="PostedFile" />
                            <img id="blah" src="#" height="80"  width="80" class="rounded-circle" />
                        </div>
                    </div>
                    <div class="row mt-4 mb-4">
                        <div class="col-md-4">
                            <h3>Personal Information</h3>
                        </div>
                        <div class="col-md-5">
                            <span style="color:red;">@TempData["InternShip"]</span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-md-4">
                                    <label>Student Name <span style="color: red">*</span></label>
                                </div>
                                <div class="col-md-8">
                                    @Html.TextBoxFor(m => m.Name, new { @class = "form-control", @placeholder = "Student Name", @onkeypress = "return onlyAlphabets();" })
                                    @*<input type="text" class="form-control"/>*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-md-4">
                                    <label>Father's Name <span style="color: red">*</span></label>
                                </div>
                                <div class="col-md-8">
                                    @Html.TextBoxFor(m => m.Father, new { @class = "form-control", @placeholder = "Father's Name", @onkeypress = "return onlyAlphabets();" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-md-4">
                                    <label>Date Of Birth <span style="color: red">*</span></label>
                                </div>
                                <div class="col-md-8">
                                    @Html.TextBoxFor(m => m.DOB, new { @class = "form-control", @placeholder = "dd/mm/yyyy", @autocomplete = "off" })
                                    @*<input type="date" class="form-control" />*@

                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 form-group">
                            <label>Postal Address <span style="color: red">*</span></label>
                            @Html.TextAreaFor(m => m.PostalAddress, new { @class = "form-control", @placeholder = "Postal Address" })
                            @*<textarea class="form-control" rows="3"></textarea>*@
                        </div>
                        <div class="col-sm-6 form-group">
                            <label>Permanent Address <span style="color: red">*</span></label>
                            @Html.TextAreaFor(m => m.PermanentAddress, new { @class = "form-control", @placeholder = "Permanent Address" })
                            @*<textarea class="form-control" rows="3"></textarea>*@
                        </div>
                        @*<div class="col-sm-2 form-group">
                                <label>Permanent Address</label>
                            </div>
                            <div class="col-sm-10 form-group">

                            </div>*@
                        <div class="col-sm-12">
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Domicile <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(m => m.Domicile, new { @class = "form-control", @placeholder = "Domicile" })
                                            @*<input type="text" class="form-control"/>*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Blood Group </label>
                                        </div>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(m => m.CellNo, new { @class = "form-control", @placeholder = "Blood Group" })
                                            @*<input type="number" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Phone No. <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(m => m.Mobile, new { @class = "form-control", @placeholder = "Phone No.", @onkeypress = "return isNumberKey(event);",@maxlength="10" })
                                            @*<input type="number" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Gender <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-8">
                                            <select name="Gender" id="Gender" class="form-control" placeholder="Gender">
                                                <option value="">-- select --</option>
                                                <option value="male">Male</option>
                                                <option value="female">Female</option>
                                                <option value="other">Other</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Religion <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(m => m.Religion, new { @class = "form-control", @placeholder = "Religion" })
                                            @*<input type="number" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-4">
                                            <label>Email <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-8">
                                            @Html.TextBoxFor(m => m.Email, new { @class = "form-control", @placeholder = "Email" })
                                            @*<input type="email" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-4 mb-4">
                        <div class="col-md-12">
                            <h3>Qualification in progress</h3>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>Degree <span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(m => m.Degree, new { @class = "form-control", @placeholder = "Degree" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>Discipline <span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(m => m.Discipline, new { @class = "form-control", @placeholder = "Discipline" })
                                    @*<input type="number" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>Semester <span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(m => m.Semester, new { @class = "form-control", @placeholder = "Semester" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>CGPA </label>
                                </div>
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(m => m.CGPA, new { @class = "form-control", @placeholder = "CGPA" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>Registration No. <span style="color: red">*</span></label>
                                </div>
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(m => m.RegistrationNo, new { @class = "form-control", @placeholder = "Registration No." })
                                    @*<input type="number" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row form-group">
                                <div class="col-sm-4">
                                    <label>Roll No. </label>
                                </div>
