                                    @Html.TextBoxFor(m => m.UniverSity, new { @class = "form-control", @placeholder = "UniverSity", @onkeypress = "return onlyAlphabets();" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="row">
                                <div class="col-sm-3">
                                    <label>Final Year Project (if any) </label>
                                </div>
                                <div class="col-sm-9">
                                    @Html.TextBoxFor(m => m.FinalYearProject, new { @class = "form-control", @placeholder = "Final Year Project (if any)" })
                                    @*<input type="text" class="form-control" />*@
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 mt-4 mb-4">
                            <h3>Previous Academic Record</h3>
                        </div>
                        <div class="col-sm-12 table-responsive">
                                <table class="table  table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Degree/Certificate <span style="color: red">*</span></th>
                                            <th>Passing Year <span style="color: red">*</span></th>
                                            <th>Marked Obtained/Total Marks & GPA <span style="color: red">*</span></th>
                                            <th>Division/Grade <span style="color: red">*</span></th>
                                            <th>Institute/Board <span style="color: red">*</span></th>
                                            <th>Action </th>
                                        </tr>
                                    </thead>
                                    <tbody  id="TextBoxContainer">
                                        <tr class="mycontainer" id="mainrow">
                                            <td>@Html.TextBoxFor(m => m.Certificate, new { @class = "Certificate form-control", @placeholder = "Degree" })</td>
                                            <td>@Html.TextBoxFor(m => m.PassingYear, new { @class = "PassingYear form-control", @placeholder = "Passing Year", @onkeypress = "return isNumberKey(event);",@maxlength="4" })</td>
                                            <td>@Html.TextBoxFor(m => m.MarksObtainedTotalMarksGPA, new { @class = "MarksObtainedTotalMarksGPA form-control", @placeholder = "Marked Obtained/Total Marks & GPA" })</td>
                                            <td>@Html.TextBoxFor(m => m.Division, new { @class = "Division form-control", @placeholder = "Division/Grade" })</td>
                                            <td>@Html.TextBoxFor(m => m.Board, new { @class = "Board form-control", @placeholder = "Institute/Board", @onkeypress = "return onlyAlphabets();" })</td>
                                            <td><input type="button" id="add" data-toggle="tooltip" data-original-title="Add" value="Add" style="width:80px" class="btn btn-success" /></td>
                                        </tr>
                                    </tbody>
                                </table>
                            <div id="orderItems">
                                <table class="table  table-striped table-bordered" id="orderdetailsItems"></table>
                                <span id="orderItemError" style="color:red"></span>
                            </div>


                        </div>
                      
                        <div class="col-sm-12 mb-4 mt-4">
                            <h5>In case applicant is son/daughter of Force employee/Ex- Force employee</h5>
                        </div>
                        <div class="col-sm-12 table-responsive">
                            <table class="table table-striped table-bordered display">
                                <thead>
                                    <tr>
                                        <th>Name of Father/Mother </th>
                                        <th>Designation </th>
                                        <th>PI. No </th>
                                        <th>Fy/Grp/Dept </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>@Html.TextBoxFor(m => m.FatherMother, new { @class = "form-control", @placeholder = "Name of Father/Mother", @onkeypress = "return onlyAlphabets();" })</td>
                                        <td>@Html.TextBoxFor(m => m.Designation, new { @class = "form-control", @placeholder = "Designation", @onkeypress = "return onlyAlphabets();" })</td>
                                        <td>@Html.TextBoxFor(m => m.PINO, new { @class = "form-control", @placeholder = "PI. No" })</td>
                                        <td>@Html.TextBoxFor(m => m.FYGRPDEPT, new { @class = "form-control Degree", @placeholder = "Fy/Grp/Dept" })</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="col-sm-12">
                            <h4 class="mt-4 mb-4">Proposed Intership Date</h4>
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="row">
                                        <div class="col-sm-2">
                                            <label>From<span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(m => m.ProPoSedInternShipFromDate, new { @class = "form-control", @placeholder = "dd/mm/yyyy", @autocomplete = "off" })
                                            @*<input type="date" class="form-control"/>*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="row">
                                        <div class="col-sm-2">
                                            <label>To <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(m => m.ProPoSedInternShipToDate, new { @class = "form-control", @placeholder = "dd/mm/yyyy", @autocomplete = "off" })
                                            @*<input type="date" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <div class="row form-group">
                                        <div class="col-sm-2">
                                            <label>Date <span style="color: red">*</span></label>
                                        </div>
                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(m => m.Date, new { @class = "form-control", @placeholder = "dd/mm/yyyy", @autocomplete = "off" })
                                            @*<input type="date" class="form-control"/>*@
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*<div class="row mt-5">
                        <div class="col-sm-6">
                            <div class="row form-group">
                                <div class="col-sm-2">
                                    <label>Date</label>
                                </div>
                                <div class="col-sm-10">
                                    @Html.TextBoxFor(m => m.Date, new { @class = "form-control", @placeholder = "dd/mm/yyyy", @autocomplete = "off" })
                                    <input type="date" class="form-control"/>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="row form-group">
                                <div class="col-sm-3">
                                    <label>Student's Signature</label>
                                </div>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>*@
                    <input type="submit" value="Submit" class="btn btn-info mt-5" id="FinalbtnSave" name="btnsave" />
                </div>
            </div>
        </div>
    </div>

    
}
